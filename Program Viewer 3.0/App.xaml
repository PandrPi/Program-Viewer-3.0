<Application x:Class="ProgramViewer3.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="TemplateResources" Source="Resources/TemplateResources.xaml" />
                <ResourceDictionary x:Name="DefaultTheme" Source="Resources/DefaultTheme.xaml" />
                <ResourceDictionary x:Name="IconResources" Source="Resources/IconResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
